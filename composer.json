{
    "name": "corncodecreators/sql-normalizer",
    "description": "Adapting SQL-strings to the best feasible standard.",
    "keywords": ["php", "mysql", "mariadb", "sql", "normalizer", "standard", "sanitizer"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CornCodeCreators\\SqlNormalizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CornCodeCreators\\SqlNormalizer\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tim Korn",
            "email": "info@corncode.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0"
    },
    "scripts": {
      "code-test": "./vendor/bin/phpunit --stop-on-failure"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "symfony/var-dumper": "^7.1",
        "corncodecreators/line-dumper": "^1.0"
    }
}
